  <Target Name="PlaceTemplates">
    <!-- Zip the Template files. -->
    <Zip Files="@(OEConsoleAppProject_Content);" Flatten="True" ZipFileName="ProjectTemplates\Built\OEConsoleAppProject.zip" />
    <Zip Files="@(OEEnclaveProject_Content);" Flatten="True" ZipFileName="ProjectTemplates\Built\OEEnclaveProject.zip" />
    <Zip Files="@(OEHostDllProject_Content);" Flatten="True" ZipFileName="ProjectTemplates\Built\OEHostDllProject.zip" />
    <!--
      Mark the Templates for copying to the VSIX file.

      The directories specified by the <Link> nodes are important. The path under "ProjectTemplates"
      is used by the VSIX installer to place the Templates in a hierarchy. Subdirectories map directly
      to the names of nodes in the Template tree. Example:

        Tree target: Visual C++ > Open Enclave App
        Path: ProjectTemplates\VC\<Template File>.zip
            (The <Name> node in the Template file is "Open Enclave App".)

        For reference, the names of language directories (CSharp, etc.) are under:
            <Program Files>\Microsoft Visual Studio 14.0\Common7\IDE\ProjectTemplates.
    -->
    <ItemGroup>
      <Content Include="ProjectTemplates\Built\OEConsoleAppProject.zip">
        <Link>ProjectTemplates\VC\OEConsoleAppProject.zip</Link>
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        <IncludeInVSIX>true</IncludeInVSIX>
      </Content>
      <Content Include="ProjectTemplates\Built\OEEnclaveProject.zip">
        <Link>ProjectTemplates\VC\OEEnclaveProject.zip</Link>
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        <IncludeInVSIX>true</IncludeInVSIX>
      </Content>
      <Content Include="ProjectTemplates\Built\OEHostDllProject.zip">
        <Link>ProjectTemplates\VC\OEHostDllProject.zip</Link>
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        <IncludeInVSIX>true</IncludeInVSIX>
      </Content>
    </ItemGroup>
  </Target>
